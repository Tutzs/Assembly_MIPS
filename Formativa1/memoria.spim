syscall
move $t0, $v0

li $v0, 5 #lendo valor de 'j' 5
syscall
move $t1, $v0

li $v0, 5 #lendo valor de 'n' 5
syscall
move $t2, $v0

li $t3, 4
# relacionado a I
mul $t0, $t0, $t3
#sll $t0, $t0, 4
add $t0, $t0, $s0

