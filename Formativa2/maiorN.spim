.data

.text
main:

li $v0, 5 # le N 5
syscall
move $t0, $v0

li $v0, 5 # lendo o 'maior' numero 
syscall
move $t1, $v0

li $t2, 1

loop:
    bge $t2, $t0, out # t2 >= t0

    li $v0, 5
    syscall
    move $t3, $v0 
    
    bgt $t3, $t1, novoMaior # t3 > t1

    addi $t2, $t2, 1

    j loop

novoMaior:
    move $t1, $t3
    addi $t2, $t2, 1
    j loop

out:
    move $a0, $t1
    li $v0, 1
    syscall

    li $v0, 11
    li $a0, 10
    syscall

    li $v0, 10
    syscall


